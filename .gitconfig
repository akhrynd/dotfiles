[user]
	name = akhrynd
	email = akhrynd@gmail.com
[alias]
	s = status
	tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
	delete-merged-branch = "!f () { git branch --merged | egrep -v '\\*|master|develop' | xargs git branch -d; }; f"
[init]
	defaultBranch = main
